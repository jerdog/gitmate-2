#!/usr/bin/env bash

export GITMATE_PROJECT_PATH=$(echo $CI_PROJECT_PATH | awk '{print tolower($0)}')
export GITMATE_IMAGE_NAME=registry.gitlab.com/$GITMATE_PROJECT_PATH
export GITMATE_CURRENT_IMAGE_TAG=$GITMATE_IMAGE_NAME:$CI_COMMIT_SHA
export GITMATE_MASTER_IMAGE_TAG=$GITMATE_IMAGE_NAME:latest
export GITMATE_RC_IMAGE_TAG=$GITMATE_IMAGE_NAME:rc
export GITMATE_RELEASE_IMAGE_TAG=$GITMATE_IMAGE_NAME:release
